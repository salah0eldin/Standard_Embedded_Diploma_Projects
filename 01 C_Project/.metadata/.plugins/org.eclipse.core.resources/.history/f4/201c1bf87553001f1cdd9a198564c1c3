#ifndef HEADERS_STDLIST_H_
#define HEADERS_STDLIST_H_

typedef struct{
	int id;
	int age;
	float gpa;
	char name[50];
} student;

typedef struct node{
	student data;
	struct node* next;
} node;

node* addStudent(student* std);

#endif /* HEADERS_STDLIST_H_ */
